; Test that an INT followed by an IRET are decoded correctly.
;
; Variant: event tracing enabled, cpl-0 tracing
;

org 0x100000
bits 64

; @pt p0: psb()
; @pt p2: mode.exec(64bit, if)
; @pt p3: psbend()
l0: nop

; @pt p4: evd(0: 0xf00baa)
; @pt p5: cfe(1: 14)
; @pt p6: tip.pge(3: %l5)
; ...
; @pt p7: cfe(2, ip)
; @pt p8: fup(1: %l5)
; @pt p9: tip.pgd(0: %l1)
l1: mov rax, [rbx]

; @pt p10: cfe(1: 0x80)
; @pt p11: tip.pge(3: %l7)
; ...
; @pt p12: cfe(2, ip)
; @pt p13: fup(1: %l7)
; @pt p14: tip.pgd(0: %l3)
l2: int 0x80
l3: nop

l5: iret
l6: hlt

l7: iret
l8: hlt


